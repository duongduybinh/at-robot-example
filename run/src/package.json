{
  "private": true,
  "scripts": {
    "build:runner": "esbuild runner.js --bundle --platform=node --format=cjs --outfile=../runner.js",
    "build:generate": "esbuild generate.js --bundle --platform=node --format=cjs --outfile=../generate.js",
    "build:test": "esbuild test.js --bundle --platform=node --format=cjs --outfile=../test.js",
    "build:json2csv": "esbuild json2csv.js --bundle --platform=node --format=cjs --outfile=../json2csv.js",
    "build:csv2json": "esbuild csv2json.js --bundle --platform=node --format=cjs --outfile=../csv2json.js",
    "build:robot": "esbuild robot.js --bundle --platform=node --format=cjs --outfile=../robot.js",
    "build": "run-p --print-label build:runner build:robot build:generate build:json2csv build:csv2json build:test"
  },
  "dependencies": {
    "@json2csv/plainjs": "^7.0.6",
    "csvtojson": "^2.0.10",
    "json2csv": "^6.0.0-alpha.2"
  },
  "devDependencies": {
    "esbuild": "^0.25.10",
    "npm-run-all": "^4.1.5"
  }
}